msgid "The universal archive manager"
msgstr "El gestor de archivos universal"

msgid "command"
msgstr "commando"

msgid "archive file to process"
msgstr "archivo a procesar"

msgid "comma separated columns to include in the listing"
msgstr "columnas separadas por comas a incluir en el listado"

msgid "unknown archive format"
msgstr "formato de archivo desconocido"

msgid "%(prog)s: warning: %(message)s\n"
msgstr "%(prog)s: aviso: %(message)s\n"

msgid "archive type and extension don't match"
msgstr "el tipo de archivo y la extensión no coinciden"

msgid "can't read %(filename)s"
msgstr "no se puede leer %(filename)s"

msgid "can't write %(filename)s"
msgstr "no se puede escribir %(filename)s"

msgid "%(filename)s is not a file"
msgstr "%(filename)s no es un archivo"

msgid "%(filename)s doesn't exist"
msgstr "%(filename)s no existe"

msgid "invalid column name: %(column)s"
msgstr "nombre de columna inválido: %(column)s"

msgid "reading %(archive)s"
msgstr "leyendo %(archive)s"

msgid "compressing into %(archive)s"
msgstr "comprimiendo en %(archive)s"

msgid "expected a list of files to compress"
msgstr "se espera una lista de archivos a comprimir"

msgid "a progress bar is already running"
msgstr "ya se ejecuta una barra de progreso"

msgid "a progress bar is not running"
msgstr "no se ejecuta ninguna barra de progreso"

msgid "PERMISSIONS"
msgstr "PERMISOS"

msgid "OWNER"
msgstr "PROPIETARIO"

msgid "SIZE"
msgstr "TAMAÑO"

msgid "DATE"
msgstr "FECHA"

msgid "NAME"
msgstr "NOMBRE"

############
# argparse #
############

msgid "usage: "
msgstr "uso: "

msgid "argument %(argument_name)s: %(message)s"
msgstr "argumento %(argument_name)s: %(message)s"

msgid ".__call__() not defined"
msgstr ".__call__() no definido"

msgid "conflicting subparser: %s"
msgstr "subanalizador en conflicto: %s"

msgid "conflicting subparser alias: %s"
msgstr "alias de subanalizador en conflicto: %s"

msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "analizador %(parser_name)r desconocido (opciones: %(choices)s)"

msgid "argument \"-\" with mode %r"
msgstr "argumento \"-\" con modo %r"

msgid "can't open '%(filename)s': %(error)s"
msgstr "no se puede abrir '%(filename)s': %(error)s"

msgid "cannot merge actions - two groups are named %r"
msgstr "no se pueden unir las acciones - dos grupos se llaman %r"

msgid "'required' is an invalid argument for positionals"
msgstr "'required' es un argumento inválido para posicionales"

msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"literal de opción inválido %(option)r: debe empezar con un carácter "
"%(prefix_chars)r"

msgid "dest= is required for options like %r"
msgstr "dest= es necesario en opciones como %r"

msgid "invalid conflict_resolution value: %r"
msgstr "valor inválido para conflict_resolution: %r"

msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "literal de opción en conflicto: %s"
msgstr[1] "literales de opción en conflicto: %s"

msgid "mutually exclusive arguments must be optional"
msgstr "los argumentos mutuamente exclusivos deben ser opcionales"

msgid "positional arguments"
msgstr "argumentos posicionales"

msgid "options"
msgstr "opciones"

msgid "show this help message and exit"
msgstr "muestra este mensaje de ayuda y termina"

msgid "cannot have multiple subparser arguments"
msgstr "no puede haber múltiples argumentos de subanalizador"

msgid "unrecognized arguments: %s"
msgstr "argumentos no conocidos: %s"

msgid "not allowed with argument %s"
msgstr "no se permite con el argumento %s"

msgid "ignored explicit argument %r"
msgstr "argumento explícito %r ignorado"

msgid "the following arguments are required: %s"
msgstr "los siguientes argumentos son necesarios: %s"

msgid "one of the arguments %s is required"
msgstr "uno de los argumentos %s es necesario"

msgid "expected one argument"
msgstr "se espera un argumento"

msgid "expected at most one argument"
msgstr "se espera a lo sumo un argumento"

msgid "expected at least one argument"
msgstr "se espera al menos un argumento"

msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "se espera el argumento %s"
msgstr[1] "se esperan los argumentos %s"

msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "opción ambigua: %(option)s podría corresponder con %(matches)s"

msgid "unexpected option string: %s"
msgstr "literal de opción inesperado: %s"

msgid "%r is not callable"
msgstr "%r no es invocable"

msgid "invalid %(type)s value: %(value)r"
msgstr "valor %(type)s inválido: %(value)r"

msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "opción inválida: %(value)r (elegir entre %(choices)s)"

msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: error: %(message)s\n"
